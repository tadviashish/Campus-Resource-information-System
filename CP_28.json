\n                let popup \u003d window.open(\"\", \"Data\", \"width\u003d600,height\u003d400\");\n                popup.document.write(\"\u003chtml\u003e\u003chead\u003e\u003ctitle\u003eData Fetched\u003c/title\u003e\u003c/head\u003e\u003cbody\u003e\");\n                popup.document.write(response.content);\n                popup.document.write(\"\u003c/body\u003e\u003c/html\u003e\");\n            } else {\n                alert(response.message);\n            }\n        }\n  \u003c/script\u003e\n  \u003c/body\u003e\n\u003c/html\u003e\n"},{"id":"906c52bc-2f18-4bb6-855b-ae5b9abe9a1a","name":"main","type":"html","source":"\u003c!DOCTYPE html\u003e\n\u003chtml\u003e\n  \u003chead\u003e\n    \u003cbase target\u003d\"_top\"\u003e\n  \u003c/head\u003e\n  \u003cbody\u003e\n    \u003c?!\u003dincludeHeader(); ?\u003e\n  \u003c/body\u003e\n\u003c/html\u003e\n"},{"id":"d66b812d-d23c-4c27-b626-03292d2c6d13","name":"admin","type":"html","source":"\u003c!DOCTYPE html\u003e\n\u003chtml\u003e\n  \u003chead\u003e\n    \u003cbase target\u003d\"_top\"\u003e\n    \u003cstyle\u003e\n        input::-webkit-outer-spin-button,\ninput::-webkit-inner-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\nul {\n  list-style-type: none;\n  margin: 0;\n  padding: 0;\n  width: 200px;\n  height:100%;\n  background-color: #f1f1f1;\n}\n\nli a {\n  display: block;\n  color: #000;\n  padding: 8px 16px;\n  text-decoration: none;\n}\n\nli a.active {\n  background-color: #04AA6D;\n  color: white;\n}\n\nli a:hover:not(.active) {\n  background-color: #555;\n  color: white;\n}\n.navbar ul{\n  height: 42vw;\n  \n}\n/* .navbar1 {\nposition: relative;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n} */\n.content{\n  display:flex;\n}\n\ninput{\n  padding: 16px;\n  margin: 30px;\n  height: 29px;\n}\n\nlabel{\n  height: 20px;\n   margin: 15px;\n}\n\n.info{\n  display: flex;\n  flex-direction: column;\n  margin-left: 8vw;\n  margin-top: 8vw;\n  align-items:flex-start;\n}\n\n\n\u003c/style\u003e\n  \u003c/head\u003e\n  \u003cbody\u003e\n    \u003cdiv class\u003d\"container\"\u003e\n    \u003c?!\u003dincludeHeader(); ?\u003e\n    \u003c/div\u003e\n    \u003cdiv class\u003d\"content\"\u003e\n    \u003cnav class\u003d\"navbar1\"\u003e\n\n  \u003cul\u003e\n    \u003cli\u003e\u003ca class\u003d\"active\" href\u003d\"#home\"\u003eHome\u003c/a\u003e\u003c/li\u003e\n    \u003cli\u003e\u003ca href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dground\"\u003eGround Floor\u003c/a\u003e\u003c/li\u003e\n    \u003cli\u003e\u003ca href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dfirst\"\u003eFirst Floor\u003c/a\u003e\u003c/li\u003e\n    \u003cli\u003e\u003ca href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dsecond\"\u003eSecond Floor\u003c/a\u003e\u003c/li\u003e\n  \u003c/ul\u003e\n\u003c/nav\u003e\n\n\u003cdiv class\u003d\"info\"\u003e\n    \u003ch2\u003eSearch Information\u003c/h2\u003e\n    \u003clabel for\u003d\"input\"\u003eSearch Faculty\u003c/label\u003e\n    \u003cdiv\u003e\n      \u003cinput type\u003d\"text\" id\u003d\"fid\" placeholder\u003d\"Enter ID\"/\u003e\n      \u003cinput class\u003d\"btn btn-primary \" type\u003d\"submit\" onclick\u003d\"finfo()\" /\u003e\n    \u003c/div\u003e\n    \n    \u003clabel for\u003d\"input\"\u003eSearch Students\u003c/label\u003e\n    \u003cdiv\u003e\n      \u003cinput type\u003d\"text\" id\u003d\"id\" placeholder\u003d\"Enter Enrollment\"/\u003e\n      \u003cinput class\u003d\"btn btn-primary bg-red\" type\u003d\"submit\" onclick\u003d\"sinfo()\" /\u003e\n    \u003c/div\u003e\n\n    \u003cdiv id\u003d\"result\"\u003e\u003c/div\u003e\n    \u003cbr\u003e\n\n  \u003c/div\u003e\n\n \u003c/div\u003e\n\n\n \u003cscript\u003e\n  function  sinfo () {\n        let txt \u003d document.querySelector(\u0027#id\u0027).value;\n        let updateLocation \u003d document.querySelector(\u0027#result\u0027);\n        //you can add other element here\n        updateLocation.innerHTML \u003d \"Getting detail...\";\n\n        function onFailure(error){\n          let warning \u003d \"\u003cspan style\u003d\u0027color:red\u0027\u003e\"+error+\"\u003c/span\u003e\";\n          updateLocation.innerHTML \u003d warning;\n        };\n        \n        function onSuccess(response){\n          let result \u003d\"\u003cspan style\u003d\u0027color:black\u0027\u003e\"+response+\"\u003c/span\u003e\";\n          updateLocation.innerHTML \u003d result;\n\n        };\n        google.script.run.withFailureHandler(onFailure)\n                         .withSuccessHandler(onSuccess)\n                         .submitDT(txt);\n\n      };\n\n      function showResult(response) {\n            if (response.success) {\n            \n                let popup \u003d window.open(\"\", \"Data\", \"width\u003d600,height\u003d400\");\n                popup.document.write(\"\u003chtml\u003e\u003chead\u003e\u003ctitle\u003eData Fetched\u003c/title\u003e\u003c/head\u003e\u003cbody\u003e\");\n                popup.document.write(response.content);\n                popup.document.write(\"\u003c/body\u003e\u003c/html\u003e\");\n            } else {\n                alert(response.message);\n            }\n        }\n\n        function  finfo () {\n        let txt \u003d document.querySelector(\u0027#fid\u0027).value;\n        let updateLocation \u003d document.querySelector(\u0027#result\u0027);\n        //you can add other element here\n        updateLocation.innerHTML \u003d \"Getting detail...\";\n\n        function onFailure(error){\n          let warning \u003d \"\u003cspan style\u003d\u0027color:red\u0027\u003e\"+error+\"\u003c/span\u003e\";\n          updateLocation.innerHTML \u003d warning;\n        };\n        \n        function onSuccess(response){\n          let result \u003d\"\u003cspan style\u003d\u0027color:black\u0027\u003e\"+response+\"\u003c/span\u003e\";\n          updateLocation.innerHTML \u003d result;\n\n        };\n        google.script.run.withFailureHandler(onFailure)\n                         .withSuccessHandler(onSuccess)\n                         .submitFData(txt);\n\n      };\n\n      function showResult(response) {\n            if (response.success) {\n                // Create a pop-up for displaying the result\n                let popup \u003d window.open(\"\", \"Data\", \"width\u003d600,height\u003d400\");\n                popup.document.write(\"\u003chtml\u003e\u003chead\u003e\u003ctitle\u003eData Fetched\u003c/title\u003e\u003c/head\u003e\u003cbody\u003e\");\n                popup.document.write(response.content);\n                popup.document.write(\"\u003c/body\u003e\u003c/html\u003e\");\n            } else {\n                alert(response.message);\n            }\n        }\n  \u003c/script\u003e\n  \u003c/body\u003e\n\u003c/html\u003e\n"},{"id":"7709e33c-f269-44ac-bb55-e57eb2f8e9a2","name":"ground","type":"html","source":"\u003c!DOCTYPE html\u003e\n\u003chtml\u003e\n\n\u003chead\u003e\n  \u003cbase target\u003d\"_top\"\u003e\n  \u003cstyle\u003e\n    ul {\n      list-style-type: none;\n      margin: 0;\n      padding: 0;\n      width: 200px;\n      height: 100%;\n      background-color: #f1f1f1;\n    }\n\n    li a {\n      display: block;\n      color: #000;\n      padding: 8px 16px;\n      text-decoration: none;\n    }\n\n    li a.active {\n      background-color: #04AA6D;\n      color: white;\n    }\n\n    li a:hover:not(.active) {\n      background-color: #555;\n      color: white;\n    }\n\n    .navbar ul {\n      height: 42vw;\n    }\n\n    .content {\n      display: flex;\n      flex-direction: row;\n      padding: 20px;\n    }\n\n    table {\n      border-collapse: collapse;\n      width: 100%;\n    }\n\n    th,\n    td {\n      border: 1px solid #ddd;\n      padding: 8px;\n    }\n\n    th {\n      background-color: #f2f2f2;\n    }\n\n    .button-container {\n      margin-top: 20px;\n    }\n\n\n    .container {\n      max-width: 800px;\n      margin: 50px auto;\n      background: #fff;\n      padding: 20px;\n      border-radius: 10px;\n      box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n    }\n\n    .section {\n      margin-bottom: 20px;\n    }\n\n    label {\n      font-size: 16px;\n      font-weight: bold;\n      display: block;\n      margin-bottom: 5px;\n    }\n\n    select {\n      width: 100%;\n      padding: 10px;\n      font-size: 16px;\n      border: 1px solid #ddd;\n      border-radius: 5px;\n      margin-bottom: 15px;\n    }\n\n    button {\n      padding: 10px 20px;\n      font-size: 16px;\n      color: #fff;\n      background-color: #007bff;\n      border: none;\n      border-radius: 5px;\n      cursor: pointer;\n      margin: 5px;\n      transition: background-color 0.3s;\n    }\n\n    button:hover {\n      background-color: #0056b3;\n    }\n\n    h3 {\n      font-size: 18px;\n      margin-bottom: 10px;\n      color: #333;\n    }\n\n  \u003c/style\u003e\n\u003c/head\u003e\n\n\u003cbody\u003e\n  \u003cdiv\u003e\n    \u003c?!\u003d includeHeader(); ?\u003e\n  \u003c/div\u003e\n  \u003cdiv class\u003d\"content\"\u003e\n    \u003cnav class\u003d\"navbar\"\u003e\n      \u003cul\u003e\n        \u003cli\u003e\u003ca href\u003d\"\u003c?\u003d myURL(); ?\u003e?page\u003dadmin\"\u003eHome\u003c/a\u003e\u003c/li\u003e\n        \u003cli\u003e\u003ca class\u003d\"active\" href\u003d\"\u003c?\u003d myURL(); ?\u003e?page\u003dground\"\u003eGround Floor\u003c/a\u003e\u003c/li\u003e\n        \u003cli\u003e\u003ca href\u003d\"\u003c?\u003d myURL(); ?\u003e?page\u003dfirst\"\u003eFirst Floor\u003c/a\u003e\u003c/li\u003e\n        \u003cli\u003e\u003ca href\u003d\"\u003c?\u003d myURL(); ?\u003e?page\u003dsecond\"\u003eSecond Floor\u003c/a\u003e\u003c/li\u003e\n      \u003c/ul\u003e\n    \u003c/nav\u003e\n    \u003cdiv class\u003d\"info\"\u003e\n      \u003cdiv class\u003d\"container\"\u003e\n        \u003cdiv class\u003d\"section\"\u003e\n          \u003clabel for\u003d\"timeSlot\"\u003eTime Slot:\u003c/label\u003e\n          \u003cselect id\u003d\"timeSlot\"\u003e\u003c/select\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"section\"\u003e\n          \u003cbutton onclick\u003d\"changeTimeSlot(\u0027prev\u0027)\"\u003ePrevious\u003c/button\u003e\n          \u003cbutton onclick\u003d\"changeTimeSlot(\u0027next\u0027)\"\u003eNext\u003c/button\u003e\n          \u003cbutton onclick\u003d\"onSubmit()\"\u003eSubmit\u003c/button\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"section\"\u003e\n          \u003ch3\u003eClasses (Ground Floor)\u003c/h3\u003e\n          \u003cul id\u003d\"classList\"\u003e\u003c/ul\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"section\"\u003e\n          \u003ch3\u003eLabs (Ground Floor)\u003c/h3\u003e\n          \u003cul id\u003d\"labList\"\u003e\u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003c/style\u003e\n  \u003cscript\u003e\n    let timeSlots \u003d [\n        \"8:30 9-15AM\",\n        \"9:15 10:00AM\",\n        \"10:00 10:10AM\",\n        \"10:10 10:55AM\",\n        \"10:55 11:40AM\",\n        \"11:40AM 12:30PM\",\n        \"12:30 1:20PM\",\n        \"1:20 2:10PM\",\n        \"2:10 2:55PM\"\n      ];\n      let currentSlotIndex \u003d 0;\n\n      function populateTimeSlots() {\n        const dropdown \u003d document.getElementById(\u0027timeSlot\u0027);\n        timeSlots.forEach((slot, index) \u003d\u003e {\n          const option \u003d document.createElement(\u0027option\u0027);\n          option.value \u003d slot;\n          option.text \u003d slot;\n          if (index \u003d\u003d\u003d 0) option.selected \u003d true;\n          dropdown.appendChild(option);\n        });\n      }\n\n      function fetchFloorData() {\n        const timeSlot \u003d timeSlots[currentSlotIndex];\n        google.script.run.withSuccessHandler(renderData).getFloorData(timeSlot);\n      }\n\n      function renderData(data) {\n        const classList \u003d document.getElementById(\u0027classList\u0027);\n        const labList \u003d document.getElementById(\u0027labList\u0027);\n        classList.innerHTML \u003d \u0027\u0027;\n        labList.innerHTML \u003d \u0027\u0027;\n\n        // Render classes\n        data.classes.forEach(c \u003d\u003e {\n          const li \u003d document.createElement(\u0027li\u0027);\n          li.innerText \u003d `${c.location} - ${c.course}`;\n          li.onclick \u003d () \u003d\u003e fetchStudents(c.location);\n          classList.appendChild(li);\n        });\n\n        // Render labs\n        data.labs.forEach(l \u003d\u003e {\n          const li \u003d document.createElement(\u0027li\u0027);\n          li.innerText \u003d `${l.location} - ${l.course}`;\n          labList.appendChild(li);\n        });\n      }\n\n      function fetchStudents(className) {\n        google.script.run.withSuccessHandler(renderStudents).getStudents(className);\n      }\n\n      function renderStudents(students) {\n        alert(`Students in the class:\\n${students.join(\u0027\\n\u0027)}`);\n      }\n\n      function changeTimeSlot(direction) {\n        if (direction \u003d\u003d\u003d \u0027prev\u0027 \u0026\u0026 currentSlotIndex \u003e 0) {\n          currentSlotIndex--;\n        } else if (direction \u003d\u003d\u003d \u0027next\u0027 \u0026\u0026 currentSlotIndex \u003c timeSlots.length - 1) {\n          currentSlotIndex++;\n        }\n        document.getElementById(\u0027timeSlot\u0027).value \u003d timeSlots[currentSlotIndex];\n      }\n\n      function onSubmit() {\n        fetchFloorData();\n      }\n\n      window.onload \u003d () \u003d\u003e {\n        populateTimeSlots();\n      };\n  \u003c/script\u003e\n\u003c/body\u003e\n\n\u003c/html\u003e"},{"id":"8cf0f4a8-80c1-4d35-8b21-dcb420b5375d","name":"first","type":"html","source":"\u003c!DOCTYPE html\u003e\n\u003chtml\u003e\n  \u003chead\u003e\n    \u003cbase target\u003d\"_top\"\u003e\n    \u003cstyle\u003e\nul {\n  list-style-type: none;\n  margin: 0;\n  padding: 0;\n  width: 200px;\n  height:100%;\n  background-color: #f1f1f1;\n}\n\nli a {\n  display: block;\n  color: #000;\n  padding: 8px 16px;\n  text-decoration: none;\n}\n\nli a.active {\n  background-color: #04AA6D;\n  color: white;\n}\n\nli a:hover:not(.active) {\n  background-color: #555;\n  color: white;\n}\n.navbar ul{\n  height: 42vw;\n}\n.content{\n  display:flex;\n}\n\u003c/style\u003e\n  \u003c/head\u003e\n  \u003cbody\u003e\n    \u003cdiv\u003e\n    \u003c?!\u003dincludeHeader(); ?\u003e\n    \u003c/div\u003e\n    \u003cdiv class\u003d\"content\"\u003e\n    \u003cnav class\u003d\"navbar\"\u003e\n  \u003cul\u003e\n    \u003cli\u003e\u003ca  href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dadmin\"\u003eHome\u003c/a\u003e\u003c/li\u003e\n    \u003cli\u003e\u003ca href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dground\"\u003eGround Floor\u003c/a\u003e\u003c/li\u003e\n    \u003cli\u003e\u003ca class\u003d\"active\" href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dfirst\"\u003eFirst Floor\u003c/a\u003e\u003c/li\u003e\n    \u003cli\u003e\u003ca href\u003d\"\u003c?\u003dmyURL(); ?\u003e?page\u003dsecond\"\u003eSecond Floor\u003c/a\u003e\u003c/li\u003e\n  \u003c/ul\u003e\n\u003c/nav\u003e\n\u003cdiv class\u003d\"info\"\u003e\n\u003ch1\u003eim first floor\u003c/h1\u003e\n\u003c/div\u003e\n\n \u003c/div\u003e\n  \u003c/body\u003e\n\u003c/html\u003e\n"},{"id":"aefec6d1-2e4e-4931-befc-b1d821e3310e","name":"second","type":"html","source":"\u003c!DOCTYPE html\u003e\n\u003chtml\u003e\n  \u003chead\u003e\n    \u003cbase target\u003d\"_top\"\u003e\n    \u003cstyle\u003e\nul {\n  list-style-type: none;\n  margin: 0;\n  padding: 0;\n  width: 200px;\n  height:100%;\n  background-color: #f1f1f1;\n}\n\nli a {\n  display: block;\n  color: #000;\n  padding: 8px 16px;\n  text-decoration: none;\n}\n\nli a.active {\n  background-color: #04AA6D;\n  color: white;\n}\n\nli a:hover:not(.active) {\n  background-color: #555;\n  color: white;\n}\n.navbar ul{\n  height: 42vw;\n}\n.content{\n  display:flex;\n}\n\u003c/style\u003e\n  \u003c/h